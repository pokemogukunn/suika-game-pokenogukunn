{
  "version": 2,
  "builds": [
    {
      "src": "js/scripts.js",
      "use": "@vercel/node"
    },
    {
      "src": "/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index2.html"
    },
    {
      "src": "/(.*)",
      "dest": "/js/scripts.js"
    }
  ]
}
